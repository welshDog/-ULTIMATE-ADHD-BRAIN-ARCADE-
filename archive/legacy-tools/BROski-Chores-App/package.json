    "prettier": "^3.2.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^7.1.12",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^4.0.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
