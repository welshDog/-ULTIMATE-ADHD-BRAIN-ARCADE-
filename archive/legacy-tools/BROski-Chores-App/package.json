{
  "name": "broski-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "homepage": "https://welshDog.github.io/BROski-Chores-App",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
